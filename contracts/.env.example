# Environment Variables for Smart Contract Deployment
# Copy to .env and fill in your values

# RPC URLs
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
POLYGON_AMOY_RPC_URL=https://rpc-amoy.polygon.technology

# Private Key (Testnet only - never use mainnet keys!)
PRIVATE_KEY=your_testnet_wallet_private_key_here

# Etherscan API Keys
ETHERSCAN_API_KEY=your_etherscan_api_key
BASE_ETHERSCAN_API_KEY=your_base_etherscan_api_key
POLYGON_ETHERSCAN_API_KEY=your_polygon_etherscan_api_key

# CoinMarketCap API (for gas reporter)
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key

# Test wallet addresses (for testing)
TEST_WALLET=0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266
USDC_TESTNET=0x...  # Sepolia USDC address